class Solution {
  public:
    // Function to search for a key in the linked list
    bool searchKey(int n, Node* head, int key) {
        Node* current = head;
        
        // Traverse the linked list
        while (current != nullptr) {
            if (current->data == key) {
                return true; // key found
            }
            current = current->next;
        }
        
        return false; // key not found
    }
};
